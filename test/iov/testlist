# iov tests
iov -datatype int -count 17 -seed 211 -iters 32768 -segments 1 -iov-order normal -overlap none
iov -datatype char -count 17 -seed 212 -iters 32768 -segments 1 -iov-order normal -overlap none
iov -datatype double -count 17 -seed 213 -iters 32768 -segments 1 -iov-order normal -overlap none
iov -datatype double_int -count 17 -seed 214 -iters 32768 -segments 1 -iov-order normal -overlap none
iov -datatype short_int -count 17 -seed 215 -iters 32768 -segments 1 -iov-order normal -overlap none
iov -datatype int:2,double:3 -count 17 -seed 216 -iters 32768 -segments 1 -iov-order normal -overlap none
iov -datatype int:3,double:2 -count 17 -seed 217 -iters 32768 -segments 1 -iov-order normal -overlap none
iov -datatype int -count 1018 -seed 218 -iters 32768 -segments 1 -iov-order normal -overlap none
